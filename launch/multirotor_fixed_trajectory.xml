<launch>
  <arg name="use_sim_time" default="false" description="Use simulation (Gazebo) clock if true"/>
  <arg name="log_level" default="WARN" description="Logging level: DEBUG, INFO, WARN, ERROR, or FATAL"/>

  <node
    pkg="corti"
    exec="multirotor_fixed_trajectory.py"
    output="screen"
    args="--ros-args --log-level $(var log_level)">
    <param name="use_sim_time" value="$(var use_sim_time)"/>
  </node>
</launch>
